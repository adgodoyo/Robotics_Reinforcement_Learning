<?xml version="1.0"?>
<launch>
    <arg name="robot_name"  default="$(env ROBOT_HOST)"/>

    <!-- startup simulated world -->
    <include file="$(find hiwonder_gazebo)/launch/empty_world.launch">
        <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/turtlebot3_autorace.world" />
        <arg name="paused"          value="false"/>
        <arg name="use_sim_time"    value="true"/>
        <arg name="gui"             value="true"/>
        <arg name="headless"        value="false"/>
        <arg name="debug"           value="false"/>
    </include>

    <include file="$(find hiwonder_gazebo)/launch/spwan_model.launch">
        <arg name="robot_name"  value="$(arg robot_name)"/>
    </include>
</launch>
